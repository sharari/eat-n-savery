<h1>New Restaurant</h1>

<%#= render 'form' %>

<%#= link_to 'Back', restaurants_path %>

<%= form_tag :action => 'create' do %>

<p><label for = "restaurants_name">Name</label>:
<%= text_field 'restaurants', 'name' %></p>

<p><label for = "restaurants_imageurl">imageurl</label>:
<%= text_field 'restaurants', 'imageurl' %></p>

<p><label for = "restaurants_socialmedia1">socialmedia1</label>:
<%= text_field 'restaurants', 'socialmedia1' %></p>

<p><label for = "restaurants_socialmedia2">socialmedia2</label>:
<%= text_field 'restaurants', 'socialmedia2' %></p>

<p><label for = "restaurants_menuurl">menuurl</label>:
<%= text_field 'restaurants', 'menuurl' %></p>

<%= submit_tag "Create" %>

<%#= collection_select(:books, :subject_id, @subjects, :id, :name, prompt: true) %><!--</p>
<p><label for = "book_description">Description</label><br/>

<%#= text_area 'books', 'description' %></p>
<%#= submit_tag "Create" %>-->

<% end -%>
<%= link_to 'Back', restaurants_path %>